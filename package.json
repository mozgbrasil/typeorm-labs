{
  "name": "typeorm-labs",
  "version": "0.0.1",
  "description": "Awesome project developed with TypeORM.",
  "devDependencies": {
    "@types/node": "^8.0.29",
    "ts-node": "3.3.0",
    "typeorm-uml": "^1.6.4",
    "typescript": "3.3.3333"
  },
  "dependencies": {
    "typeorm": "0.2.41",
    "reflect-metadata": "^0.1.10",
    "pg": "^8.4.0",
    "express": "^4.15.4",
    "body-parser": "^1.18.1"
  },
  "scripts": {
    "start": "ts-node src/index.ts",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "migration:create": "typeorm migration:create -n MultiTables",
    "migration:run": "npm run typeorm -- migration:run",
    "migration:revert": "npm run typeorm -- migration:revert",
    "db:diagram": "ts-node ./node_modules/.bin/typeorm-uml ormconfig.json"
  }
}
